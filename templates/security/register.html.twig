{% extends 'layout.html.twig' %}
    {% block container %}
        <h1>S'enregistrer</h1>

        {{ form_start(form) }}
        {{ form_row(form.userName) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.confirm_password) }}
        <button type="submit" class="btn btn-success">Valider</button>
        {{ form_end(form) }}
    {% endblock %} 